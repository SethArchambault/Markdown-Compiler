    height:90%%;
}
.nav-button {
    display:none;
}
@media(max-width:1100px) {
    .nav-button {
        display:block;
        font-weight:bold; top:10px;
        padding:10px 10px 16px;
        text-align:center;
        background:rgba(255,255,255,0.9);
        color:#e31c60;
        font-size:30px;
    }
    .main-container {
        flex-direction:column;
    }
    .side-nav {
        box-sizing:border-box;
        padding:20px;
        display:none;
    }
    .nav-wrapper{
        position:fixed;
        top:10px;
    }
    .nav-wrapper:hover .side-nav {
        position:fixed;
        top:20px;
        display:block;
        background:rgba(255,255,255,0.9);
    }
    .nav-wrapper:hover .nav-button {
            display:none;
    }
    .content-box {
        margin: 0 auto;
    }
    .side-nav-spacer {
        display:none;
    }
    img {
        display:none;
    }
}
%s
